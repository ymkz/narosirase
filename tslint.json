{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["src/json/*.json"]
  },
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "no-console": false,
    "interface-name": false,
    "jsx-no-lambda": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "prettier": [
      true,
      {
        "printWidth": 100,
        "semi": false,
        "singleQuote": true
      }
    ]
  }
}
