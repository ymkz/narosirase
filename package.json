{
  "private": true,
  "version": "2.0.0",
  "name": "narosirase",
  "main": "node_modules/expo/AppEntry.js",
  "homepage": "https://ymkz.co/work#narosirase",
  "description": "A web-novel reader for Syosetuka-ni-Narou.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ymkz/narosirase"
  },
  "bugs": {
    "url": "https://github.com/ymkz/narosirase/issues"
  },
  "scripts": {
    "start": "expo start",
    "lint": "tslint --project .",
    "lint:fix": "tslint --fix --project .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "dayjs": "^1.7.2",
    "expo": "^28.0.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-28.0.0.tar.gz",
    "react-native-cheerio": "^1.0.0-rc.4",
    "react-native-router-flux": "^4.0.0-beta.31",
    "react-native-swipe-list-view": "^1.3.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "rn-snackbar": "^1.0.5",
    "tslib": "^1.9.3",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^0.4.5",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/expo": "^27.0.6",
    "@types/expo__vector-icons": "^6.2.2",
    "@types/react": "^16.4.6",
    "@types/react-native": "^0.55.28",
    "@types/react-redux": "^6.0.3",
    "prettier": "^1.13.7",
    "react-native-typescript-transformer": "^1.2.10",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.9.2"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit $GIT_PARAMS",
      "pre-commit": "yarn lint && yarn typecheck",
      "post-merge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && yarn || :"
    }
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "ignoreDeps": [
      "react",
      "react-native"
    ],
    "minor": {
      "groupName": "all dependencies"
    },
    "schedule": "every weekend",
    "timezone": "Asia/Tokyo"
  }
}
