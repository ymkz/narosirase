{
  "name": "github",
  "version": "0.1.0",
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "private": true,
  "scripts": {
    "start": "react-native-scripts start",
    "ios": "react-native-scripts ios",
    "android": "react-native-scripts android",
    "eject": "react-native-scripts eject",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "prettier-eslint --write 'src/**/*.js'",
    "lint:check": "eslint --print-config .eslintrc.yaml | eslint-config-prettier-check",
    "clean": "rimraf node_modules && yarn",
    "clean:cache": "rimraf node_modules && yarn cache clean && yarn"
  },
  "dependencies": {
    "expo": "^24.0.2",
    "react": "16.0.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-24.0.0.tar.gz"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.15.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-react-native": "^3.2.1",
    "husky": "^0.15.0-beta.16",
    "jest-expo": "^24.0.0",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.0",
    "react-native-scripts": "1.8.1",
    "react-test-renderer": "16.0.0",
    "rimraf": "^2.6.2"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit $GIT_PARAMS",
      "pre-commit": "yarn run lint",
      "post-merge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && yarn || :"
    }
  }
}
